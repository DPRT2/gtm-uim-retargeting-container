// Vorlagencode hier eingeben.
const log = require('logToConsole');
const injectScript = require('injectScript');
const queryPermission = require('queryPermission');
const setInWindow = require('setInWindow');

log('data: ', data);

// todo dev, qa, prod
const containerUrl = 'https://s.uicdn.com/t/prod/iq/uim/' + data.uim_container_name + '/dprt.js';

const dprtData = {
  event_type : data.event,
  page_name : '',

  uuid_netid : '',

  privacy_law : 'gdpr',
  privacy_consent : '1'
};

/* if (queryPermission('get_url', 'host')) {
  const host = getUrl('host');
  log(host);
} */

switch (data.event) {
  case 'general':
    dprtData.page_name = data.page_name;
    break;
  case 'category':
    dprtData.category_id = '';
    dprtData.category_name = '';
    break;
  case 'product':
	dprtData.product_id = data.product_id;
	dprtData.product_name = data.product_name;
	dprtData.product_price = data.product_price;
	dprtData.product_currency = data.currency;
	break;
  case 'basket':
	dprtData.basket_total = data.basket_total;
	dprtData.basket_items = data.product_id;
	dprtData.basket_currency = data.currency;
	break;
  case 'checkout':
	dprtData.order_id = data.order_id;
	dprtData.order_items = data.product_id;
	dprtData.order_total = data.order_total;
	dprtData.order_shipping = data.order_shipping;
	dprtData.order_currency = data.currency;
	break;
}

function injectSuccess() {
  log('injectSuccess');
  data.gtmOnSuccess();
}

function injectFailure() {
  log('injectFailure');
  data.gtmOnFailure();
}

log('dprtData: ', dprtData);
log('containerUrl: ', containerUrl);

if (queryPermission('access_globals', 'write', 'dprt_data')) {
  
  setInWindow('dprt_data', dprtData, true);
  
  if (queryPermission('inject_script', containerUrl)) {
    log('queryPermission');
    injectScript(containerUrl, injectSuccess, injectFailure);
  } else {
    log('inject_script fail, possibly permissions mismatch.');
    data.gtmOnFailure();
  }
  
} else {
  log('write dprt_data fail');
  data.gtmOnFailure();
}


